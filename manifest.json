{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "Weather In France",
    "short_name": "Weather",
    "description": "Read the title  /Flaticon.com'. This page has been designed using resources from Flaticon.com/",
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "icons": {
        "48": "./images/10d@2x.png",
        "128": "./images/10d@2x.png"
    },
    "browser_action": {
        "browser_style": false,
        "default_title": "Weather",
        "default_popup": "index.html",
        "default_icon": {
            "24": "./images/10d@2x.png",
            "32": "./images/10d@2x.png"
        }
    },
    "background": {
        "scripts": []
    },
    "web_accessible_resources": [],
    "permissions": [
        "storage"
    ]
}